{
  "name": "@awesomesolana/app",
  "version": "0.1.0",
  "files": [
    "src"
  ],
  "scripts": {
    "analyze:unused": "next-unused",
    "analyze": "cross-env ANALYZE=true yarn run build",
    "dev": "yarn && next dev",
    "clean": "rm -rf .next .vercel tsconfig.tsbuildinfo",
    "build": "yarn && yarn clean && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "format:check": "prettier -c .",
    "test": "jest --clearCache && jest",
    "test:watch": "jest --watch",
    "checks": "yarn typecheck && yarn test && yarn build",
    "format": "prettier -w .",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@awesomesolana/common": "workspace:^",
    "@primer/octicons-react": "^16.3.1",
    "@supabase/supabase-js": "^1.30.7",
    "@tailwindcss/typography": "^0.5.2",
    "axios": "^0.26.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "lodash.delay": "^4.1.1",
    "lodash.times": "^4.3.2",
    "mobx": "^6.4.2",
    "mobx-react-lite": "^3.3.0",
    "next": "^12.1.0",
    "next-seo": "^5.1.0",
    "next-themes": "^0.1.1",
    "next-transpile-modules": "^9.0.0",
    "plur": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^1.2.2",
    "tailwind-merge": "^1.2.0",
    "use-debounce": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@next/bundle-analyzer": "^12.1.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/lodash.delay": "^4.1.6",
    "@types/lodash.times": "^4.3.6",
    "@types/node": "^17.0.21",
    "@types/plur": "^3.1.0",
    "@types/react": "^17.0.39",
    "@types/tailwindcss": "^3.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "27.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-docblock": "27.5.1",
    "jest-runner": "27.5.1",
    "jest-runner-groups": "2.1.0",
    "next-sitemap": "^2.5.2",
    "next-unused": "^0.0.6",
    "postcss": "^8.4.7",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "react-test-renderer": "^17.0.2",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.5.0",
    "typescript": "4.6.2"
  },
  "next-unused": {
    "alias": {
      "@/*": [
        "src/*"
      ]
    },
    "debug": true,
    "include": [
      "src",
      "src/modules",
      "src/ui"
    ],
    "exclude": [
      ".test.",
      ".unit.",
      ".spec."
    ],
    "entrypoints": [
      "src/pages",
      "src/modules"
    ]
  }
}
